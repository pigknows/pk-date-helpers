{
  "name": "pk-date-helpers",
  "version": "1.7.3",
  "description": "date conversions used throughout pigknows products",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pigknows/pk-date-helpers"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --config=webpack.config.js -p",
    "test": "jest --notify"
  },
  "keywords": [
    "node",
    "date",
    "conversions"
  ],
  "author": "inkblotty",
  "license": "ISC",
  "dependencies": {
    "luxon": "^1.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-stage-3": "7.0.0-beta.52",
    "@babel/preset-typescript": "^7.0.0-beta.52",
    "@types/jest": "^23.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.2.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "jest": "^23.3.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "webpack": "^3.12.0",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "setupFiles": [
      "../scripts/test-setup.js"
    ],
    "rootDir": "src",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/*.ts?(x)",
      "**/?(*.)+test.ts?(x)"
    ],
    "globals": {
      "babel-jest": {
        "useBabelrc": true,
        "tsConfigFile": "../jest.tsconfig.json"
      }
    }
  }
}
